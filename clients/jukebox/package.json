{
  "name": "@cats-cradle/jukebox",
  "description": "An API for the audio service",
  "module": "commonjs",
  "version": "0.0.2",
  "scripts": {
    "dev": "nest start --debug --watch",
    "build": "tsc",
    "build:all": "rush build -t .",
    "artifact": "create-artifact @cats-cradle/jukebox",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "lint": "eslint --format visualstudio \"./{stacks,src}/**/*.ts\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./{stacks,src}/*.ts\"  --fix-dry-run",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@cats-cradle/constructs": "workspace:*",
    "@cats-cradle/base-nodejs": "workspace:*",
    "aws-lambda": "1.0.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/terminus": "9.1.1",
    "@nestjs/platform-express": "9.3.9",
    "aws-serverless-express": "~3.4.0",
    "@vendia/serverless-express": "4.12.6",
    "express": "4.18.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "@nestjs/swagger": "~7.1.17",
    "uuid": "~9.0.1",
    "@cats-cradle/nestjs-modules": "workspace:*",
    "@cats-cradle/validation-schemas": "workspace:*",
    "@nestjs/config": "2.2.0",
    "@nestjs/serve-static": "~3.0.1"
  },
  "devDependencies": {
    "aws-cdk-lib": "2.129.0",
    "constructs": "^10.2.70",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.5",
    "@types/aws-lambda": "~8.10.130",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "@microsoft/tsdoc": "~0.13.2",
    "typescript": "5.3.3",
    "ts-node": "10.9.2",
    "esbuild": "~0.17.5",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "@types/express": "~4.17.21",
    "@types/uuid": "~9.0.7",
    "eslint": "8.56.0",
    "@cats-cradle/eslint-config": "1.0.11",
    "@nestjs/testing": "9.2.1",
    "supertest": "~6.3.3",
    "@types/supertest": "~2.0.16",
    "@cats-cradle/faker-factory": "workspace:*",
    "@fastify/static": "~6.9.0",
    "@cats-cradle/create-bundle": "1.0.12",
    "@cats-cradle/create-artifact": "2.0.7"
  }
}
