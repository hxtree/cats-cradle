{
  "name": "@cats-cradle/api-gateway",
  "module": "commonjs",
  "version": "0.1.2",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "dev": "nest start --debug --watch",
    "artifact": "create-artifact @cats-cradle/api-gateway",
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "lint": "eslint --format visualstudio \"./{stacks,src}/*.{ts,tsx}\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./{stacks,src}/*.{ts,tsx}\"  --fix-dry-run",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "aws-lambda": "1.0.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/terminus": "9.1.1",
    "@nestjs/platform-express": "9.3.9",
    "aws-serverless-express": "~3.4.0",
    "@vendia/serverless-express": "4.10.4",
    "express": "4.18.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "mongoose": "6.9.1",
    "source-map-support": "~0.5.21",
    "@nestjs/swagger": "~7.1.13",
    "uuid": "~9.0.1",
    "@aws-cdk/core": "~1.182.0",
    "constructs": "^10.2.70",
    "aws-cdk-lib": "2.95.0",
    "construct": "~1.0.0"
  },
  "devDependencies": {
    "@cats-cradle/create-artifact": "2.0.0",
    "@cats-cradle/constructs": "workspace:*",
    "@cats-cradle/base-nodejs": "workspace:*",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.2",
    "@types/aws-lambda": "~8.10.123",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "typescript": "5.1.3",
    "ts-node": "10.9.1",
    "esbuild": "~0.17.5",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "@types/express": "~4.17.18",
    "@types/uuid": "~9.0.6",
    "eslint": "8.44.0",
    "@cats-cradle/eslint-config": "1.0.6"
  }
}
