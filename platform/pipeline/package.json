{
  "name": "@cats-cradle/pipeline",
  "module": "commonjs",
  "version": "1.0.0",
  "description": "An API for simulating dice rolls and luck from dice notation",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "cdk": "cdk",
    "hotswap": "cdk deploy --hotswap",
    "lint": "",
    "lint:ci": "",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:bootstrap": "cdk bootstrap",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@cats-cradle/constructs": "workspace:*",
    "aws-lambda": "1.0.7",
    "eslint": "8.44.0",
    "@cats-cradle/base-nodejs": "workspace:*",
    "@microsoft/rush-lib": "5.100.1",
    "@rushstack/node-core-library": "~3.61.0"
  },
  "devDependencies": {
    "@cats-cradle/eslint-config": "1.0.6",
    "aws-cdk-lib": "2.95.0",
    "constructs": "^10.2.70",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.2",
    "@types/aws-lambda": "~8.10.123",
    "jest": "29.7.0",
    "typescript": "5.1.3",
    "ts-node": "10.9.1",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "lodash": "~4.17.21",
    "@types/lodash": "~4.14.200"
  }
}
