{
  "name": "@cats-cradle/pipeline",
  "module": "commonjs",
  "version": "1.0.0",
  "description": "An API for simulating dice rolls and luck from dice notation",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test:local": "jest",
    "cdk": "cdk",
    "hotswap": "cdk deploy --hotswap",
    "lint": "",
    "lint:ci": "",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@cats-cradle/constructs": "workspace:*",
    "aws-lambda": "1.0.7",
    "eslint": "8.26.0",
    "@cats-cradle/base-nodejs": "workspace:*",
    "@microsoft/rush-lib": "~5.89.1",
    "@rushstack/node-core-library": "~3.55.2"
  },
  "devDependencies": {
    "@cats-cradle/eslint-config": "workspace:*",
    "aws-cdk-lib": "2.47.0",
    "constructs": "^10.1.211",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/aws-lambda": "~8.10.93",
    "jest": "28.1.3",
    "typescript": "4.9.5",
    "ts-node": "10.9.1",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "lodash": "~4.17.21",
    "@types/lodash": "~4.14.191"
  }
}
