{
  "name": "@cats-cradle/message-bus",
  "module": "commonjs",
  "version": "2.1.0",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "dev": "nest start --debug --watch",
    "artifact": "create-artifact @cats-cradle/message-bus",
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "lint": "eslint --format visualstudio \"./{stacks,src}/*.{ts,tsx}\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./{stacks,src}/*.{ts,tsx}\"  --fix-dry-run",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-cdk/core": "~1.182.0",
    "@cats-cradle/messaging-schemas": "2.1.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/swagger": "~7.1.17",
    "@nestjs/terminus": "9.1.1",
    "@vendia/serverless-express": "4.12.6",
    "aws-cdk-lib": "2.129.0",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "~3.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "construct": "~1.0.0",
    "constructs": "^10.2.70",
    "express": "4.19.2",
    "mongoose": "6.11.3",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "uuid": "~9.0.1"
  },
  "devDependencies": {
    "@cats-cradle/base-nodejs": "1.0.11",
    "@cats-cradle/constructs": "2.1.0",
    "@cats-cradle/create-artifact": "2.0.7",
    "@cats-cradle/eslint-config": "1.0.11",
    "@types/aws-lambda": "~8.10.130",
    "@types/express": "~4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.25",
    "@types/uuid": "~9.0.7",
    "aws-cdk": "2.129.0",
    "esbuild": "~0.20.1",
    "eslint": "8.57.0",
    "fs": "~0.0.2",
    "jest": "29.7.0",
    "path": "~0.12.7",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
