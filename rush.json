{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.58.0",
  "pnpmVersion": "6.7.1",
  "pnpmOptions": {
    "pnpmStore": "local",
    "strictPeerDependencies": true,
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=16.13.1",
  "suppressNodeLtsWarning": false,
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "allowMostlyStandardPackageNames": false,
  "approvedPackagesPolicy": {
    "reviewCategories": [
      "production",
      "tools",
      "prototypes"
    ],
    "ignoredNpmScopes": [
      "@types"
    ]
  },
  "gitPolicy": {
    "allowedEmailRegExps": [
      "[^@]+@gmail\\.com"
    ],
    "sampleEmail": "mrexample@gmail.com",
    "versionBumpCommitMessage": "Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "Update changelogs [skip ci]"
  },
  "repository": {
    "url": "https://github.com/hxtree/monorepo",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [
      "common/scripts/pre-rush-install.js"
    ],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "telemetryEnabled": false,
  "hotfixChangeEnabled": false,
  "projects": [
    {
      "packageName": "helloworld",
      "projectFolder": "apps/helloworld",
      "reviewCategory": "production"
    },
    {
      "packageName": "base-node-rig",
      "projectFolder": "rigs/base-node-rig",
      "reviewCategory": "production"
    }
  ]
}