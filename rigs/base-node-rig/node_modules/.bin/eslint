#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/usr/src/app/common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/node_modules:/usr/src/app/common/temp/node_modules:/usr/src/app/common/node_modules:/usr/src/app/node_modules:/usr/src/node_modules:/usr/node_modules:/node_modules:/usr/src/app/apps/heft-node-rig-tutorial/node_modules:/usr/src/app/apps/node_modules"
else
  export NODE_PATH="$NODE_PATH:/usr/src/app/common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/node_modules:/usr/src/app/common/temp/node_modules:/usr/src/app/common/node_modules:/usr/src/app/node_modules:/usr/src/node_modules:/usr/node_modules:/node_modules:/usr/src/app/apps/heft-node-rig-tutorial/node_modules:/usr/src/app/apps/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../common/temp/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/eslint.js" "$@"
fi
