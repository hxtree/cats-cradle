  "info": {
    "title": "@cats-cradles/audio-service",
    "description": "An API for the audio service",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "QueryDto": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "description": "Date",
            "default": "2023-11-20T03:28:08.392Z"
          },
          "latitude": {
            "type": "string",
            "description": "Latitude",
            "default": "38.2942"
          },
          "longitude": {
            "type": "string",
