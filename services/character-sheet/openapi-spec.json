  "info": {
    "title": "@cats-cradles/character-sheet",
    "description": "An API for the character sheet service",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "CreateCharacterSheetDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "default": "afdd27df-e56f-42b5-a76b-668bb791375b"
          },
          "instanceId": {
            "type": "string",
            "default": "f1641d0f-9595-4677-9949-b29dbceaf3e7"
          },
          "archetypeId": {
            "type": "string",
            "enum": [
              "DARUMA_NAKAMURA",
              "MOTHER",
              "MISCHIEVOUS_PIEBALD",
              "JANUS_PERSIAN",
              "LOOMEE_ANGORA",
              "OUERN_ONI",
              "VALLON_ONI",
              "MEEKU_ONI",
              "ARINOTH_DIYATH",
              "FAYE_IMAGO",
              "GAALI_RUNEWIN",
              "GUNTER_STONEWELL",
              "MALACE_TSIA",
              "PENNY_KIBBUTZ",
              "TRAEZ_UTHSHA",
              "VOID",
              "GENKI",
              "LAWZON_GREY",
              "MADHI_TSIA",
              "SUYRI",
              "WISP",
              "ASMIN",
              "DIAG",
              "MONARCH",
              "VICEROY",
              "WARLING",
              "SPIDER",
              "SEA_HORSE",
              "SENSITIVE_PLANT",
              "DEEP_THINKER",
              "GEL",
              "PHOENIX",
              "SNOW_LEOPARD",
              "CHIMERA",
              "SLOW_MOVER",
              "SNAKE_OF_THE_WATER",
              "BANDIT",
              "ROWAN",
              "CHUBBS"
            ],
            "default": "DARUMA_NAKAMURA"
          },
          "name": { "type": "string", "default": "Daruma" },
          "surname": { "type": "string", "default": "Nakamura" }
        },
        "required": ["_id", "instanceId", "archetypeId", "name", "surname"]
      },
      "CreateSpawnDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "default": "1077d648-a6aa-468b-86f7-788e42421f90"
          },
          "instanceId": {
            "type": "string",
            "default": "7b6744b0-f364-4833-9c5d-2273361f32b1"
          },
          "place": {
            "type": "string",
            "enum": [
              "HOSTIA",
              "PORT",
              "FORT",
              "FLOATING_ISLAND",
              "INFINITY_ISLAND",
              "ENDERS_LAND",
              "IFM_TRAIN"
            ],
            "description": "Generate a spawn for a designated area. Requestor is responsible for only making request for areas where party is presence to avoid computing irrelevant spawns."
          }
        },
        "required": ["id", "instanceId", "place"]
      },
      "UpdateAffiliationDto": {
        "type": "object",
        "properties": {
          "characterSheetId": {
            "type": "string",
            "default": "88623540-0638-4e3d-8e15-a86792290835"
          },
          "affiliationId": {
            "type": "string",
            "enum": [
              "Reputation",
              "THE_DESTROYERS",
              "THE_CATS",
              "ARCHANGELS",
              "DEVILS",
              "GUARDIANS",
              "VALLONS_SEVEN",
              "BROTHERHOOD",
              "SOLIDER_FORCES",
              "X_SOLIDER_FORCES",
              "MAGI_ORDER",
              "MAGI_ORDER_ELDERS",
              "CATCHERS",
              "REBEL",
              "ORIGINALS",
              "OCEANIA",
              "EURASIA",
              "BARBARIANS",
              "NINJA",
              "LEGION_SEE",
              "FLIPPING_PROFITEERS"
            ],
            "default": "THE_CATS"
          },
          "value": { "type": "number" },
          "operation": {
            "type": "string",
            "enum": ["ADD", "REMOVE"],
            "default": "ADD"
          }
        },
        "required": ["characterSheetId", "affiliationId", "value", "operation"]
      }
    }
  }
