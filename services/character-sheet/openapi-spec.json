  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "CreateCharacterSheetDto": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "archetypeId": {
            "type": "string",
            "enum": [
              "MOTHER",
              "MISCHIEVOUS_PIEBALD",
              "JANUS_PERSIAN",
              "LOOMEE_ANGORA",
              "OUERN_ONI",
              "VALLON_ONI",
              "MEEKU_ONI",
              "ARINOTH_DIYATH",
              "FAYE_IMAGO",
              "GAALI_RUNEWIN",
              "GUNTER_STONEWELL",
              "MALACE_TSIA",
              "PENNY_KIBBUTZ",
              "TRAEZ_UTHSHA",
              "VOID",
              "GENKI",
              "LAWZON_GREY",
              "MADHI_TSIA",
              "SUYRI",
              "WISP",
              "ASMIN",
              "DIAG",
              "MONARCH",
              "VICEROY",
              "SPIDER",
              "WARLING",
              "SEA_HORSE",
              "SENSITIVE_PLANT",
              "DEEP_THINKER",
              "PHOENIX",
              "SNOW_LEOPARD",
              "CHIMERA",
              "SLOW_MOVER",
              "SNAKE_OF_WATER"
            ]
          },
          "name": { "type": "string" },
          "surname": { "type": "string" }
        },
        "required": ["id", "archetypeId", "name", "surname"]
      },
      "CreateSpawnDto": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "instanceId": { "type": "string" },
          "place": {
            "type": "string",
            "enum": [
              "HOSTIA",
              "PORT",
              "FORT",
              "FLOATING_ISLAND",
              "INFINITY_ISLAND",
              "ENDERS_LAND"
            ],
            "description": "Generate a spawn for a designated area. Requestor is responsible for only making request for areas where party is presence to avoid computing irrelevant spawns."
          }
        },
        "required": ["id", "instanceId", "place"]
      }
    }
