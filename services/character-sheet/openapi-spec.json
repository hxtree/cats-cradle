  "info": {
    "title": "@galaxyops/character-sheet",
    "description": "A service for maintaining character sheets",
    "version": "3.1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "CreateCharacterSheetDto": {
        "type": "object",
        "properties": {
          "instanceId": {
            "type": "string",
            "default": "7b7bde99-de96-45fa-8b4d-05e171af3100"
          },
          "archetypeId": {
            "type": "string",
            "default": "DARUMA_NAKAMURA",
            "enum": [
              "DARUMA_NAKAMURA",
              "MOTHER",
              "MISCHIEVOUS_PIEBALD",
              "JANUS_PERSIAN",
              "LOOMEE_ANGORA",
              "OUERN_ONI",
              "VALLON_ONI",
              "MEEKU_ONI",
              "ARINOTH_DIYATH",
              "FAYE_IMAGO",
              "GAALI_RUNEWIN",
              "GUNTER_STONEWELL",
              "MALACE_TSIA",
              "PENNY_KIBBUTZ",
              "TRAEZ_UTHSHA",
              "VOID",
              "GENKI",
              "LAWZON_GREY",
              "MADHI_TSIA",
              "SUYRI",
              "WISP",
              "ASMIN",
              "DIAG",
              "MONARCH",
              "VICEROY",
              "WARLING",
              "SPIDER",
              "SEA_HORSE",
              "SENSITIVE_PLANT",
              "DEEP_THINKER",
              "GEL",
              "PHOENIX",
              "SNOW_LEOPARD",
              "CHIMERA",
              "SLOW_MOVER",
              "SNAKE_OF_THE_WATER",
              "BANDIT",
              "ROWAN",
              "CHUBBS"
            ]
          },
          "name": { "type": "string", "default": "Daruma" },
          "surname": { "type": "string", "default": "Nakamura" }
        },
        "required": ["instanceId", "archetypeId", "name", "surname"]
      },
      "CreateSpawnDto": {
        "type": "object",
        "properties": {
          "instanceId": {
            "type": "string",
            "default": "ac548402-a20d-42d7-9a60-c6fb9cceb70c"
          },
          "place": {
            "type": "string",
            "description": "Generate a spawn for a designated area. Requestor is responsible for only making request for areas where party is presence to avoid computing irrelevant spawns.",
            "enum": [
              "HOSTIA",
              "PORT",
              "FORT",
              "FLOATING_ISLAND",
              "INFINITY_ISLAND",
              "ENDERS_LAND",
              "IFM_TRAIN"
            ]
          }
        },
        "required": ["instanceId", "place"]
      },
      "UpdateAffiliationDto": {
        "type": "object",
        "properties": {
          "characterSheetId": {
            "type": "string",
            "default": "0261eb67-ae3f-40bd-9c52-f19c9a8a1d97"
          },
          "affiliationId": {
            "type": "string",
            "default": "THE_CATS",
            "enum": [
              "THE_DESTROYERS",
              "THE_CATS",
              "THE_ANTIFELINES",
              "ARCHANGELS",
              "DEVILS",
              "GUARDIANS",
              "VALLONS_SEVEN",
              "BROTHERHOOD",
              "SOLIDER_FORCES",
              "X_SOLIDER_FORCES",
              "MAGI_ORDER",
              "MAGI_ORDER_ELDERS",
              "CATCHERS",
              "REBEL",
              "ORIGINALS",
              "OCEANIA",
              "EURASIA",
              "BARBARIANS",
              "NINJA",
              "LEGION_SEE",
              "FLIPPING_PROFITEERS"
            ]
          },
          "value": { "type": "number" },
          "operation": {
            "type": "string",
            "default": "ADD",
            "enum": ["ADD", "REMOVE"]
          }
        },
        "required": ["characterSheetId", "affiliationId", "value", "operation"]
      }
    }
  }
