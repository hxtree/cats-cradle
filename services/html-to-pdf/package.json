{
  "name": "@cats-cradle/html-to-pdf",
  "module": "commonjs",
  "version": "1.0.4",
  "description": "An API Simulating dice rolls that can be effected by luck from dice notation",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build:project": "bundle-project @cats-cradle/luck-by-dice",
    "dev": "nest start --debug --watch",
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint --format visualstudio \"./src/**/*.ts\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./src/**/*.ts\"  --fix-dry-run",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "nestjs-bundler && cdk deploy",
    "cdk:hotswap": "nestjs-bundler && cdk deploy --hotswap",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@cats-cradle/constructs": "workspace:*",
    "@cats-cradle/base-nodejs": "workspace:*",
    "aws-lambda": "1.0.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/terminus": "9.1.1",
    "@nestjs/platform-express": "9.3.9",
    "aws-serverless-express": "~3.4.0",
    "@vendia/serverless-express": "4.10.4",
    "express": "4.18.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "@nestjs/swagger": "~7.1.13",
    "eslint": "8.44.0",
    "@cats-cradle/validation-schemas": "workspace:*",
    "@nestjs/testing": "9.2.1",
    "@nestjs/axios": "~2.0.0",
    "@cats-cradle/faker-factory": "workspace:*",
    "@sparticuz/chromium": "112.0.0",
    "puppeteer-core": "19.8.0",
    "uuid": "~9.0.1"
  },
  "devDependencies": {
    "@cats-cradle/eslint-config": "1.0.6",
    "aws-cdk-lib": "2.95.0",
    "constructs": "^10.2.70",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.2",
    "@types/aws-lambda": "~8.10.123",
    "jest": "29.7.0",
    "typescript": "5.1.3",
    "ts-node": "10.9.1",
    "esbuild": "~0.17.5",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "@types/express": "~4.17.18",
    "@types/uuid": "~9.0.4",
    "lodash": "~4.17.21",
    "@types/lodash": "~4.14.191",
    "supertest": "~6.3.3",
    "@types/supertest": "~2.0.14",
    "@cats-cradle/bundlers": "0.0.3",
    "@cats-cradle/bundle-project": "1.1.1"
  }
}
