{
  "name": "@cats-cradle/design-system",
  "private": false,
  "version": "0.5.1",
  "module": "commonjs",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "dev": "storybook dev -p 3000 -s public",
    "build": "tsc --build ./tsconfig-build.json && vite build",
    "build:storybook": "tsc --p ./tsconfig-build.json && storybook build",
    "build:all": "rush build -t .",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "watch": "tsc -w",
    "lint": "eslint --format visualstudio \"./src/**/*.tsx\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./src/**/*.tsx\"  --fix-dry-run",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "storybook build && cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@emotion/react": "~11.11.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "~6.4.2",
    "@fortawesome/free-solid-svg-icons": "~6.4.2",
    "@cats-cradle/base-nodejs": "workspace:*",
    "jsdom": "~23.0.1",
    "lodash": "~4.17.21",
    "bootstrap": "~5.3.2"
  },
  "devDependencies": {
    "vite-plugin-checker": "0.6.2",
    "react-day-picker": "8.9.1",
    "react-intl": "6.5.5",
    "react-router-dom": "6.20.1",
    "@types/react-modal": "3.16.3",
    "@types/styled-components": "~5.1.30",
    "react-google-charts": "~4.0.1",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "8.52.0",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "glob": "^10.3.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.3",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "@mui/lab": "~5.0.0-alpha.134",
    "@mui/material": "~5.12.2",
    "@mui/system": "~5.12.1",
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "11.11.0",
    "react-syntax-highlighter": "~15.5.0",
    "@fortawesome/react-fontawesome": "~0.2.0",
    "ts-node": "10.9.1",
    "@cats-cradle/eslint-config": "1.0.8",
    "autoprefixer": "~10.4.16",
    "constructs": "^10.2.70",
    "aws-cdk-lib": "2.95.0",
    "@types/react-syntax-highlighter": "~15.5.10",
    "@babel/core": "7.23.5",
    "@babel/preset-env": "7.23.5",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@storybook/addon-a11y": "7.6.3",
    "@storybook/addon-actions": "7.6.3",
    "@storybook/addon-essentials": "7.6.3",
    "@storybook/addon-interactions": "7.6.3",
    "@storybook/addon-links": "7.6.3",
    "@storybook/builder-vite": "7.6.3",
    "@storybook/node-logger": "7.6.3",
    "@storybook/react": "7.6.3",
    "@storybook/react-vite": "7.6.3",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.5.8",
    "axios": "^0.21.1",
    "jest": "29.7.0",
    "sass": "^1.47.0",
    "storybook": "7.6.3",
    "ts-jest": "29.1.1",
    "uuid": "~9.0.1",
    "@storybook/addon-styling": "~1.3.7",
    "@types/lodash": "~4.14.200",
    "identity-obj-proxy": "~3.0.0"
  },
  "watch": {
    "copy-files": {
      "patterns": [
        "src"
      ],
      "extensions": "css,scss",
      "runOnChangeOnly": true
    }
  }
}
