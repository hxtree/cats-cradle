{
  "name": "@cats-cradle/luck-by-dice",
  "module": "commonjs",
  "version": "1.0.4",
  "description": "An API Simulating dice rolls that can be effected by luck from dice notation",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "start": "nest start --debug --watch",
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint --format visualstudio \"./src/**/*.ts\" --fix",
    "lint:ci": "eslint --format visualstudio \"./src/**/*.ts\" --fix-dry-run",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "nestjs-bundler && cdk deploy",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@cats-cradle/constructs": "workspace:*",
    "@cats-cradle/base-nodejs": "workspace:*",
    "aws-lambda": "1.0.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@nestjs/common": "9.3.10",
    "@nestjs/core": "9.2.1",
    "@nestjs/terminus": "9.1.1",
    "@nestjs/platform-express": "9.3.9",
    "aws-serverless-express": "~3.4.0",
    "@vendia/serverless-express": "4.8.2",
    "express": "4.18.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.6",
    "source-map-support": "~0.5.21",
    "@nestjs/swagger": "~6.1.3",
    "eslint": "8.26.0",
    "@cats-cradle/validation-schemas": "workspace:*",
    "@nestjs/testing": "9.2.1",
    "@nestjs/axios": "~2.0.0"
  },
  "devDependencies": {
    "@cats-cradle/eslint-config": "workspace:*",
    "aws-cdk-lib": "2.47.0",
    "constructs": "^10.1.303",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/aws-lambda": "~8.10.114",
    "jest": "29.5.0",
    "typescript": "5.0.2",
    "ts-node": "10.9.1",
    "esbuild": "~0.17.5",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "@types/express": "~4.17.14",
    "@types/uuid": "~8.3.4",
    "lodash": "~4.17.21",
    "@types/lodash": "~4.14.191",
    "supertest": "~6.3.3",
    "@types/supertest": "~2.0.12",
    "@cats-cradle/bundlers": "workspace:*"
  }
}
