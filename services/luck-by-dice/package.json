{
  "name": "@cats-cradle/luck-by-dice",
  "module": "commonjs",
  "version": "2.0.11",
  "description": "An API Simulating dice rolls that can be effected by luck from dice notation",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "artifact": "create-artifact @cats-cradle/luck-by-dice",
    "bundle": "create-bundle --watch",
    "dev": "nest start --debug --watch",
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest --runInBand",
    "test:cov": "jest --coverage --runInBand",
    "lint": "eslint --format visualstudio \"./src/**/*.ts\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./src/**/*.ts\"  --fix-dry-run",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@cats-cradle/constructs": "workspace:*",
    "@cats-cradle/base-nodejs": "workspace:*",
    "aws-lambda": "1.0.7",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@nestjs/common": "9.4.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/terminus": "9.1.1",
    "@nestjs/platform-express": "9.3.9",
    "aws-serverless-express": "~3.4.0",
    "@vendia/serverless-express": "4.10.4",
    "express": "4.18.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "@nestjs/swagger": "~7.1.16",
    "eslint": "8.56.0",
    "@cats-cradle/validation-schemas": "workspace:*",
    "@nestjs/testing": "9.2.1",
    "@nestjs/axios": "~2.0.0"
  },
  "devDependencies": {
    "@cats-cradle/eslint-config": "1.0.8",
    "aws-cdk-lib": "2.95.0",
    "constructs": "^10.2.70",
    "@types/jest": "29.5.11",
    "@types/node": "20.9.0",
    "@types/aws-lambda": "~8.10.130",
    "jest": "29.7.0",
    "typescript": "5.3.3",
    "ts-node": "10.9.2",
    "esbuild": "~0.17.5",
    "fs": "~0.0.1-security",
    "path": "~0.12.7",
    "@types/express": "~4.17.21",
    "@types/uuid": "~9.0.7",
    "lodash": "~4.17.21",
    "@types/lodash": "~4.14.202",
    "supertest": "~6.3.3",
    "@types/supertest": "~2.0.16",
    "@cats-cradle/create-bundle": "1.0.7",
    "@cats-cradle/create-artifact": "2.0.0"
  }
}
