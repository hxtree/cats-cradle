{
  "name": "@cats-cradle/authentication-service",
  "module": "commonjs",
  "version": "1.0.7",
  "scripts": {
    "dev": "nest start --debug --watch",
    "build": "tsc",
    "build:all": "rush build -t .",
    "artifact": "create-artifact @cats-cradle/authentication-service",
    "watch": "tsc -w",
    "test": "jest --logHeapUsage",
    "test:cov": "jest --coverage",
    "lint": "eslint --format visualstudio \"./{stacks,src}/**/*.ts\" --fix",
    "lint:ci": "eslint --format visualstudio \"./{stacks,src}/*.ts\" --fix-dry-run",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth",
    "cdk:hotswap": "cdk deploy --hotswap",
    "cdk:deploy": "cdk deploy",
    "cdk:watch": "cdk watch",
    "cdk:destroy": "cdk destroy",
    "depcheck": "npx depcheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cats-cradle/base-nodejs": "workspace:*",
    "@cats-cradle/constructs": "workspace:*",
    "@cats-cradle/nestjs-modules": "workspace:*",
    "@cats-cradle/validation-schemas": "workspace:*",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/mongoose": "9.2.1",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/swagger": "~7.1.13",
    "@nestjs/terminus": "9.1.1",
    "@vendia/serverless-express": "4.10.4",
    "aws-lambda": "1.0.7",
    "aws-serverless-express": "~3.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "express": "4.18.2",
    "node-cache": "5.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "source-map-support": "~0.5.21",
    "uuid": "~9.0.1",
    "mongoose": "6.9.1",
    "@nestjs/passport": "9.0.0",
    "aws-sdk": "~2.1469.0",
    "@nestjs/jwt": "~10.1.1",
    "passport": "~0.6.0",
    "passport-jwt": "~4.0.1",
    "@types/passport-jwt": "~3.0.10"
  },
  "devDependencies": {
    "@cats-cradle/create-artifact": "2.0.0",
    "@cats-cradle/eslint-config": "1.0.6",
    "@cats-cradle/faker-factory": "workspace:*",
    "@microsoft/tsdoc": "~0.13.2",
    "@nestjs/testing": "9.2.1",
    "@types/aws-lambda": "~8.10.123",
    "@types/express": "~4.17.18",
    "@types/jest": "29.5.5",
    "@types/node": "20.8.2",
    "@types/supertest": "~2.0.14",
    "@types/uuid": "~9.0.6",
    "aws-cdk-lib": "2.95.0",
    "constructs": "^10.2.70",
    "esbuild": "~0.17.5",
    "eslint": "8.44.0",
    "fs": "~0.0.1-security",
    "jest": "29.7.0",
    "path": "~0.12.7",
    "supertest": "~6.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "@cats-cradle/create-bundle": "1.0.2",
    "@shelf/jest-mongodb": "~4.1.7",
    "@types/passport": "~1.0.13"
  }
}
