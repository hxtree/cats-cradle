{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.2.1",
    "generators": {
      "character-sheet": {
        "output": "./libraries/api-client/character-sheet",
        "inputSpec": "./packages/character-sheet/openapi-spec.json",
        "ignoreFileOverride": "./common/config/openapitools/.openapi-generator-ignore",
        "generatorName": "typescript-axios",
        "templateDir": "./common/config/openapitools/templates/typescript-axios",
        "additionalProperties": {
          "npmName": "@catscradle/api-client-character-sheet",
          "npmVersion": "1.0.0",
          "npmRepository": "https://github.com/hxtree/cats-cradle",
          "withInterfaces": true,
          "stringEnums": true
        }
      },
      "luck-by-dice": {
        "output": "./libraries/api-client/luck-by-dice",
        "inputSpec": "./packages/luck-by-dice/openapi-spec.json",
        "ignoreFileOverride": "./common/config/openapitools/.openapi-generator-ignore",
        "generatorName": "typescript-axios",
        "templateDir": "./common/config/openapitools/templates/typescript-axios",
        "additionalProperties": {
          "npmName": "@catscradle/api-client-luck-by-dice",
          "npmVersion": "1.0.0",
          "npmRepository": "https://github.com/hxtree/cats-cradle",
          "withInterfaces": true,
          "stringEnums": true
        }
      }
    }
  }
}
