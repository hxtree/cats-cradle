{
  "name": "@cats-cradle/nestjs-modules",
  "version": "0.2.5",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hxtree/cats-cradle.git"
  },
  "keywords": [
    "nestjs",
    "aws",
    "s3",
    "mongo",
    "sns",
    "sqs"
  ],
  "bugs": {
    "url": "https://github.com/hxtree/cats-cradle/issues"
  },
  "scripts": {
    "build": "tsc",
    "build:all": "rush build -t .",
    "watch": "tsc -w",
    "test": "jest --logHeapUsage",
    "test:cov": "jest --coverage",
    "lint": "eslint --format visualstudio \"./{stacks,src}/*.ts\"  --fix",
    "lint:ci": "eslint --format visualstudio \"./{stacks,src}/*.ts\"  --fix-dry-run",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "@cats-cradle/base-nodejs": "workspace:*",
    "reflect-metadata": "0.1.13",
    "@types/validator": "~13.7.10",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.3.9",
    "@nestjs/mongoose": "9.2.1",
    "mongoose": "6.9.1",
    "connection-string": "~4.3.6",
    "uuid": "~9.0.1",
    "ts-mongoose-pagination": "~1.1.2",
    "rxjs": "7.8.1",
    "rimraf": "~4.1.2",
    "dotenv": "~16.0.3",
    "@aws-sdk/client-s3": "3.289.0",
    "@aws-sdk/client-sns": "3.289.0",
    "@cats-cradle/messaging-schemas": "workspace:*",
    "@types/uuid": "~9.0.6",
    "luxon": "~3.3.0",
    "mongodb-memory-server-global": "8.13.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "20.8.2",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "typescript": "5.1.3",
    "ts-node": "10.9.1",
    "esbuild": "~0.17.5",
    "eslint": "8.44.0",
    "@cats-cradle/eslint-config": "1.0.6",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "9.2.1",
    "@types/mongoose": "~5.11.97",
    "aws-sdk-client-mock": "~2.1.0",
    "@aws-sdk/types": "3.289.0",
    "@cats-cradle/faker-factory": "workspace:*",
    "@types/luxon": "~3.2.0",
    "@shelf/jest-mongodb": "~4.1.7",
    "mongodb": "6.0.0"
  },
  "config": {
    "mongodbMemoryServer": {
      "platform": "linux",
      "arch": "x64",
      "version": "4.4.5",
      "md5Check": "1"
    }
  }
}
