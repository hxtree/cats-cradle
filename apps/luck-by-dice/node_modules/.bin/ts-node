#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/usr/src/app/common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules/ts-node/dist/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules/ts-node/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/node_modules:/usr/src/app/common/temp/node_modules:/usr/src/app/common/node_modules:/usr/src/app/node_modules:/usr/src/node_modules:/usr/node_modules:/node_modules:/usr/src/app/apps/luck-by-dice/node_modules:/usr/src/app/apps/node_modules"
else
  export NODE_PATH="$NODE_PATH:/usr/src/app/common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules/ts-node/dist/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules/ts-node/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/node_modules:/usr/src/app/common/temp/node_modules:/usr/src/app/common/node_modules:/usr/src/app/node_modules:/usr/src/node_modules:/usr/node_modules:/node_modules:/usr/src/app/apps/luck-by-dice/node_modules:/usr/src/app/apps/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules/ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../../../../common/temp/node_modules/.pnpm/ts-node@10.4.0_31a32bd81d8bbfc3c2c321de918dd405/node_modules/ts-node/dist/bin.js" "$@"
fi
