#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/usr/src/app/common/temp/node_modules/.pnpm/jest@27.4.5/node_modules/jest/bin/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/jest@27.4.5/node_modules/jest/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/jest@27.4.5/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/node_modules:/usr/src/app/common/temp/node_modules:/usr/src/app/common/node_modules:/usr/src/app/node_modules:/usr/src/node_modules:/usr/node_modules:/node_modules:/usr/src/app/apps/helloworld/node_modules/jest/bin/node_modules:/usr/src/app/apps/helloworld/node_modules/jest/node_modules:/usr/src/app/apps/helloworld/node_modules:/usr/src/app/apps/node_modules"
else
  export NODE_PATH="$NODE_PATH:/usr/src/app/common/temp/node_modules/.pnpm/jest@27.4.5/node_modules/jest/bin/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/jest@27.4.5/node_modules/jest/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/jest@27.4.5/node_modules:/usr/src/app/common/temp/node_modules/.pnpm/node_modules:/usr/src/app/common/temp/node_modules:/usr/src/app/common/node_modules:/usr/src/app/node_modules:/usr/src/node_modules:/usr/node_modules:/node_modules:/usr/src/app/apps/helloworld/node_modules/jest/bin/node_modules:/usr/src/app/apps/helloworld/node_modules/jest/node_modules:/usr/src/app/apps/helloworld/node_modules:/usr/src/app/apps/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
